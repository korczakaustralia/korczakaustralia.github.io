          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".card-wrapper, .card-wrapper .card-box, .card-wrapper .card-img": {
              "position": "relative"
            },
            ".card-wrapper when (@showBorders)": {
              "border": "1px solid @bordersColor",
              ".mbr-overlay": {
                "background": "transparent",
                "opacity": "1"
              }
            },
            ".card-wrapper .mbr-overlay when not (@transparentBg)": {
              "background": "@cardBg",
              "opacity": "@cardOpacity"
            },
            ".card-img when (@showIcons)": {
              "display": "inline-block",
              "width": "auto",
              ".amp-iconfont": {
                "font-size": "~\"@{iconsSize}rem\"",
                "width": "~\"@{iconsSize}rem\"",
                "color": "@iconsColor !important",
                "& when ((@iconsSize * 16) > 40)": {
                  "@media (max-width: 767px)": {
                    "font-size": "40px",
                    "width": "40px"
                  }
                }
              }
            },
            ".card-wrapper when not (@transparentBg), (@showBorders), (@showCardsEffect)": {
              "@media (max-width: 991px)": {
                "padding": "15px"
              }
            },
            ".card-title when (@showLine) and (@showCardsTitle)": {
              "display": "inline-block",
              "&:after": {
                "content": "\"\"",
                "display": "block",
                "background-color": "@primaryColor",
                "height": "3px",
                "width": "25px",
                "margin": "inherit",
                "margin-top": "15px",
                "& when (@cardsAlign = '3')": {
                  "@media (max-width: 767px)": {
                    "margin-right": "auto"
                  }
                }
              }
            },
            ".card-wrapper when (@showLineEffect) and (@showLine) and (@showCardsTitle)": {
              ".card-title:after": {
                "-webkit-transition": "all .2s ease",
                "transition": "all .2s ease"
              },
              "&:hover .card-title:after": {
                "width": "100%"
              }
            },
            "& when (@showCardsEffect)": {
              ".card-wrapper > *, .card-wrapper, .amp-iconfont, .card-box > *, .card-title:after, svg": {
                "-webkit-transition": "all 0.3s linear",
                "transition": "all 0.3s linear"
              },
              ".card-wrapper:hover when not (@showBorders) and (@transparentBg)": {
                "& when (@effect = \"1\")": {
                  "-webkit-box-shadow": "0px 0px 40px 1px rgba(0, 0, 0, 0.1)",
                  "box-shadow": "0px 0px 40px 1px rgba(0, 0, 0, 0.1)"
                },
                "& when (@effect = \"2\")": {
                  "background-color": "@successColor"
                },
                ".card-img .amp-iconfont, .card-box .card-title": {
                  "color": "@primaryColor !important"
                },
                "svg": {
                  "fill": "@primaryColor !important"
                }
              },
              ".card-wrapper:hover when (@showBorders), not (@transparentBg)": {
                ".mbr-overlay": {
                  "background": "@primaryColor"
                },
                ".card-img .amp-iconfont, .card-box > *": {
                  "color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important",
                  "svg": {
                    "fill": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%)"
                  }
                },
                ".card-box .btn[class*=\"-outline\"], .card-box .btn[class*=\"-outline\"]:hover": {
                  "color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important",
                  "border-color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important"
                },
                ".card-box .card-title:after when (@showCardsTitle) and (@showLine)": {
                  "background-color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important"
                }
              }
            },
            ".mbr-row when (@showBorders) and (@showBorderTop) and not (@showPadding)": {
              "border-top": "1px solid @bordersColor"
            },
            ".mbr-row .card when not (@showPadding)": {
              "padding": "0",
              "& when (@showBorders)": {
                ".card-wrapper": {
                  "border": "none"
                },
                "@media (min-width: 768px)": {
                  "&:not(.last-child) .card-wrapper": {
                    "border-right": "1px solid @bordersColor"
                  },
                  "@media (max-width: 991px)": {
                    "&.md-pb .card-wrapper when (@cardsAmount = 4)": {
                      "border-bottom": "1px solid @bordersColor"
                    },
                    "&.md-pb:nth-child(2) .card-wrapper when (@cardsAmount = 4)": {
                      "border-right": "none"
                    }
                  }
                },
                "@media (max-width: 767px)": {
                  "&:not(.last-child) .card-wrapper": {
                    "border-right": "none",
                    "border-bottom": "1px solid @bordersColor"
                  }
                }
              }
            },
            ".card-title": {
              "text-align": "center"
            },
            ".mbr-text": {
              "text-align": "center"
            }
