              ".card-title:after": {
                "-webkit-transition": "all .2s ease",
                "transition": "all .2s ease"
              },
              "&:hover .card-title:after": {
                "width": "100%"
              }
            },
            "& when (@showCardsEffect)": {
              ".card-wrapper > *, .card-wrapper, .amp-iconfont, .card-box > *, .card-title:after, svg": {
                "-webkit-transition": "all 0.3s linear",
                "transition": "all 0.3s linear"
              },
              ".card-wrapper:hover when not (@showBorders) and (@transparentBg)": {
                "& when (@effect = \"1\")": {
                  "-webkit-box-shadow": "0px 0px 40px 1px rgba(0, 0, 0, 0.1)",
